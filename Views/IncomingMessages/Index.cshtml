                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SmsMessageSid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NumMedia)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ToCity)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FromZip)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SmsSid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FromState)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SmsStatus)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FromCity)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FromCountry)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ToZip)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NumSegments)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MessageSid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ApiVersion)
                </td>
                <td>
        <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
        <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
        <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
    </td>
            </tr>
}
        </tbody>
    </table>
