                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FromCity)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FromCountry)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ToZip)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NumSegments)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MessageSid)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ApiVersion)
                </th>
                <!--<th></th>-->
            </tr>
        </thead>
        <tbody>
