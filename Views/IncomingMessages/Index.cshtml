@model IEnumerable<SymJumbles.Models.IncomingMessage>
    @{
    ViewData["Message"] = "Recorded Data from Incoming API Calls via Twilio";
    }
    <h2>Index</h2>
    <h3>@ViewData["Message"]</h3>
    <!--<p>
            <a asp-action="Create">Create New</a>
        </p>-->
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.From)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.To)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Body)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AccountSid)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ToCountry)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ToState)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SmsMessageSid)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NumMedia)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ToCity)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FromZip)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SmsSid)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FromState)
                </th>
                <th>
